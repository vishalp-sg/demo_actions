<odoo>
    <data>
        <record id="view_subject_form" model="ir.ui.view">
            <field name="name">subject.form</field>
            <field name="model">university.subject</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="department"/>
                            <field name="credits"/>
                            <field name="prerequisites"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>
