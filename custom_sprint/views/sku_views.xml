<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_cohorts_sku_tree" model="ir.ui.view">
            <field name="name">view.cohorts.sku.tree</field>
            <field name="model">cohorts.sku</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name"/>
                </tree>
            </field>
        </record>

         <record id="action_cohorts_sku" model="ir.actions.act_window">
            <field name="name">Cohorts SKUs</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">cohorts.sku</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem name="Cohorts SKUs"
            id="menu_cohorts_sku"
            action="action_cohorts_sku"
            parent="menu_students_config"
            sequence="1"/>

        <record id="view_cohorts_fees_sku_tree" model="ir.ui.view">
            <field name="name">view.cohorts.fees.sku.tree</field>
            <field name="model">cohorts.fees.sku</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name"/>
                </tree>
            </field>
        </record>

         <record id="action_cohorts_fees_sku" model="ir.actions.act_window">
            <field name="name">Cohorts Fees SKUs</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">cohorts.fees.sku</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem name="Cohorts Fees SKUs"
            id="menu_cohorts_fees_sku"
            action="action_cohorts_fees_sku"
            parent="menu_students_config"
            sequence="2"/>

    </data>
</odoo>
